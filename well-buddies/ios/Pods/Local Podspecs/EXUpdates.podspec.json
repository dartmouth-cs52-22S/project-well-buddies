{
  "name": "EXUpdates",
  "version": "0.13.1",
  "summary": "Fetches and manages remotely-hosted assets and updates to your app's JS bundle.",
  "description": "Fetches and manages remotely-hosted assets and updates to your app's JS bundle.",
  "license": "MIT",
  "authors": "650 Industries, Inc.",
  "homepage": "https://docs.expo.dev/versions/latest/sdk/updates/",
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.4",
  "source": {
    "git": "https://github.com/expo/expo.git"
  },
  "static_framework": true,
  "dependencies": {
    "ExpoModulesCore": [

    ],
    "React-Core": [

    ],
    "EXStructuredHeaders": [

    ],
    "EXUpdatesInterface": [

    ],
    "EXManifests": [

    ],
    "EASClient": [

    ],
    "ASN1Decoder": [
      "~> 1.8"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_TREAT_INCOMPATIBLE_POINTER_TYPE_WARNINGS_AS_ERRORS": "YES",
    "GCC_TREAT_IMPLICIT_FUNCTION_DECLARATIONS_AS_ERRORS": "YES",
    "DEFINES_MODULE": "YES",
    "SWIFT_COMPILATION_MODE": "wholemodule"
  },
  "source_files": "EXUpdates/**/*.{h,m,swift}",
  "script_phases": {
    "name": "Generate app.manifest for expo-updates",
    "script": "bash -l -c \"$PODS_TARGET_SRCROOT/../scripts/create-manifest-ios.sh\"",
    "execution_position": "before_compile"
  },
  "resource_bundles": {
    "EXUpdates": [

    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/*.{h,m,swift}",
      "resources": "Tests/Support/**/*",
      "dependencies": {
        "OCMockito": [
          "~> 6.0"
        ]
      },
      "pod_target_xcconfig": {
        "GCC_TREAT_INCOMPATIBLE_POINTER_TYPE_WARNINGS_AS_ERRORS": "YES",
        "GCC_TREAT_IMPLICIT_FUNCTION_DECLARATIONS_AS_ERRORS": "YES",
        "DEFINES_MODULE": "YES",
        "SWIFT_COMPILATION_MODE": "wholemodule"
      }
    }
  ],
  "swift_version": "5.4"
}
